Package: DNNSIM
Type: Package
Title: Single-Index Neural Network for Skewed Heavy-Tailed Data
Version: 0.1.1
Authors@R: c(
    person("Qingyang", "Liu", email = "rh8liuqy@gmail.com", role = c("aut", "cre"), comment = c(ORCID = "0000-0003-3265-6330")),
